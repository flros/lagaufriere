
<%= simple_form_for(@rentable, :html => {:multipart => true}) do |f| %>
<%= f.error_notification %>
<div class="form-inputs">
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.input :title %>
  <%= f.input :price %>
  <%= f.label :picture %>
  <%= f.file_field :picture %>

</div>

<br>

<div class="form-actions">

  <%= f.button :submit %>
</div>
<% end %>